<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
<ScrollView

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <LinearLayout
        android:id="@+id/one"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/item_title"
            android:text="CRICKET"
            android:textColor="#117751"
            android:layout_gravity="center"
            android:textSize="30sp"
            />
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#119"/>


        <TextView
           android:id="@+id/facultyname"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Faculty Co-ordinator"
            android:gravity="center"
            android:textStyle="bold"
            android:textSize="20sp"
            android:padding="12dp"

            android:textColor="@color/textColorHeadings"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/facultycoordinator0ne"
            android:padding="5dp"
            android:orientation="horizontal">


            <ImageView
                android:id="@+id/facultyimage"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/edit1" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="NAME"
                    android:textColor="#119"
                    android:textSize="15dp"/>
                <TextView
                    android:id="@+id/name1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:padding="10dp"
                    android:text="Mr. abc"
                    android:textSize="15dp" />


            </LinearLayout>


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/facultycoordinatorTwo"
            android:textColor="#000000"
            android:padding="5dp"
            android:orientation="horizontal">


            <ImageView
                android:id="@+id/facultyimage2"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/edit1" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="NAME"
                    android:textColor="#119"
                    android:textSize="15dp"/>

                <TextView
                    android:id="@+id/name2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:padding="10dp"
                    android:text="Mr. abc"
                    android:textSize="15dp" />


            </LinearLayout>


        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Student Co-ordinator(s)"
            android:gravity="center"
            android:textStyle="bold"
            android:textSize="20sp"
            android:padding="12dp"
            android:textColor="@color/textColorHeadings"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:id="@+id/studentcoordinator0ne"
            android:padding="10dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imagestudent1"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/edit1" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="5dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="NAME"
                    android:textColor="#119"
                    android:textSize="15dp"/>
                <TextView
                    android:id="@+id/studentname1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:padding="5dp"
                    android:text="Mr. abc"
                    android:textSize="15dp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Mob No."
                    android:textColor="#119"
                    android:textSize="15dp"/>
                <TextView
                    android:id="@+id/studentnumber1"
                    android:textColor="#000000"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:text="" />
            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/studentcoordinatortwo"
            android:textColor="#000000"
            android:padding="5dp"
            android:visibility="visible"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imagestudent2"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/edit1" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="NAME"
                    android:textColor="#119"
                    android:textSize="15dp"/>
                <TextView
                    android:id="@+id/studentname2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:padding="5dp"
                    android:text="Mr. abc"
                    android:textSize="15dp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Mob No."
                    android:textColor="#119"
                    android:textSize="15dp"/>
                <TextView
                    android:id="@+id/studentnumber2"
                    android:layout_width="wrap_content"
                    android:textColor="#000000"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:text="" />
            </LinearLayout>


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="5dp"
            android:id="@+id/studentcoordinatorthree"
            android:visibility="visible"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imagestudent3"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/edit1" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="5dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="NAME"
                    android:textColor="#119"
                    android:textSize="15dp"/>
                <TextView
                    android:id="@+id/studentname3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:text="Mr. abc"
                    android:textColor="#000000"
                    android:textSize="15dp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Mob No."
                    android:textColor="#119"
                    android:textSize="15dp"/>
                <TextView
                    android:id="@+id/studentnumber3"
                    android:textColor="#000000"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:text="" />
            </LinearLayout>


        </LinearLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/type"
            android:text="Type: "
            android:layout_gravity="center"
            android:textStyle="bold"
            android:textSize="20dp"
            android:textColor="#119"
            android:paddingTop="5dp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:id="@+id/type_detail"
            android:layout_gravity="center"
            android:padding="10dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/rule"
            android:paddingTop="5dp"
            android:text="Rules: "
            android:textSize="20dp"
            android:layout_gravity="center"
            android:textStyle="bold"
            android:textColor="#119"

            />
      <!--  <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:textSize="15dp"
            android:id="@+id/detail_rules"

            android:padding="5dp"/>-->
        <WebView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/detail_rules"
            android:padding="5dp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="venue :"
            android:textColor="#119"
            android:layout_gravity="center"
            android:textSize="20dp"
            android:textStyle="bold"
            android:id="@+id/d_v"
            android:layout_below="@+id/detail_rules"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/detail_v_d"
            android:text="05/10/16 | Ab5"
            android:textStyle="bold"
            android:textColor="#000000"
            android:paddingTop="5dp"
            android:layout_gravity="center"
            android:layout_alignTop="@+id/d_v"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="20dp"
           />


    </LinearLayout>

</ScrollView>
</RelativeLayout>